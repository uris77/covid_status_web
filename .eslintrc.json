{
  "plugins": ["@typescript-eslint", "react", "import", "prettier"],
  "parserOptions": {
    "project": "./tsconfig.json",
    "sourceType": "module",
    "ecmaVersion": 2016,
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "extends": ["prettier", "plugin:@typescript-eslint/recommended"],
  "ignorePatterns": ["node_modules/"],
  "rules": {
    "prettier/prettier": ["error"]
  }
}
